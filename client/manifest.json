{
    "description": "Anti-Phishing Extension",
    "manifest_version": 2,
    "name": "AntiPhish",
    "version": "0.1",
    
    "permissions": [
        "webRequest",
        "<all_urls>",
        "tabs"
    ],
  
    "background": {
        "scripts": ["background.js"]
    },
 
    "browser_action": {
        "default_title": "AntiPhish",
        "browser_style": true,
        "default_icon": {
            "16": "icons/gray_16.png",
            "32": "icons/gray_32.png",
            "48": "icons/gray_48.png"
        },
        "default_popup": "popups/info.html"
    }
}